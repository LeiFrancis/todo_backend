<% include top %>
<form method='POST' action='/notesave'>
    <input type='hidden' name='docreate' value='<%= docreate ? "create" : "update"%>'>
    <input type="hidden" name="id" value='<%= id? id : ""  %>'>
    <p>Key: <input type='text' name='notekey' value='<%= notekey ? notekey : "" %>'></p>
    <p>Title: <input type='text' name='title' value='<%= title_note ? title_note : "" %>'></p>
    <br/>
    <textarea rows=5 cols=40 name='body'><%= note ? note : "" %></textarea>
    <br/>
    <input type='submit' value='Submit' />
</form>
<% include bottom %>